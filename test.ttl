            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&food;hasDrink"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&vin;hasColor"/>
                        <owl:hasValue rdf:resource="&food;Red"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#ConsumableThing -->

    <owl:Class rdf:about="&food;ConsumableThing"/>
    


