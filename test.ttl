        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&food;madeFromFruit"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#LightMeatFowl -->

