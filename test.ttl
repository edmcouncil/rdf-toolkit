    <owl:Ontology rdf:about="http://www.w3.org/TR/2003/PR-owl-guide-20031209/food">
        <rdfs:comment>
      Derived from the DAML Wine ontology at 
      http://ontolingua.stanford.edu/doc/chimaera/ontologies/wines.daml
      Substantially modified.
    </rdfs:comment>
        <owl:imports rdf:resource="http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#course -->

    <owl:ObjectProperty rdf:about="&food;course">
        <rdfs:domain rdf:resource="&food;Meal"/>
        <rdfs:range rdf:resource="&food;MealCourse"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#hasDrink -->

    <owl:ObjectProperty rdf:about="&food;hasDrink">
        <rdfs:domain rdf:resource="&food;MealCourse"/>
        <rdfs:range rdf:resource="&food;PotableLiquid"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#hasFood -->

    <owl:ObjectProperty rdf:about="&food;hasFood">
        <rdfs:range rdf:resource="&food;EdibleThing"/>
        <rdfs:domain rdf:resource="&food;MealCourse"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#madeFromFruit -->

    <owl:ObjectProperty rdf:about="&food;madeFromFruit">
        <rdfs:domain rdf:resource="&food;ConsumableThing"/>
        <rdfs:range rdf:resource="&food;Fruit"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#BlandFish -->

    <owl:Class rdf:about="&food;BlandFish">
        <rdfs:subClassOf rdf:resource="&food;Fish"/>
        <owl:disjointWith rdf:resource="&food;NonBlandFish"/>
    </owl:Class>
    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#BlandFishCourse -->

    <owl:Class rdf:about="&food;BlandFishCourse">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&food;MealCourse"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&food;hasFood"/>
                        <owl:allValuesFrom rdf:resource="&food;BlandFish"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&food;hasDrink"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&vin;hasFlavor"/>
                        <owl:hasValue rdf:resource="&food;Delicate"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#CheeseNutsDessert -->

    <owl:Class rdf:about="&food;CheeseNutsDessert">
